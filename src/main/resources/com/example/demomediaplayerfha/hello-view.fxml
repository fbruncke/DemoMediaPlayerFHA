<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.VBox?>


<?import javafx.scene.control.Button?>
<?import javafx.scene.media.MediaView?>
<VBox alignment="CENTER" spacing="20.0" xmlns:fx="http://javafx.com/fxml"
      fx:controller="com.example.demomediaplayerfha.HelloController"
      prefHeight="325.0" prefWidth="434.0">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0"/>
    </padding>

    <Label fx:id="Mediaplayer"/>
    <Button text="Hello!" onAction="#onHelloButtonClick"/>

    <MediaView fx:id="mediaV" fitHeight="200.0" fitWidth="200.0" />

    <Button fx:id="play" mnemonicParsing="false" onAction="#handlePlay" text="Play"  />
</VBox>
